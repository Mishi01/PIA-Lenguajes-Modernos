{
    "version":2,
    "builds" :[
        {
            "src":"*.js",
            "use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"/"
        }
    ],
        "headers": [
          {
            "source": "/(.*)",
            "headers": [
              {
                "key": "Access-Control-Allow-Origin",
                "value": "https://friendly-baklava-919e9c.netlify.app/"
              }
            ]
          }
        ]
} 