{
    "version":2,

    "routes":[
        {
            "src":"/(.*)",
            "dest":"/"
        }
    ],
        "headers": [
          {
            "source": "/(.*)",
            "headers": [
              {
                "key": "Access-Control-Allow-Origin",
                "value": "https://friendly-baklava-919e9c.netlify.app/"
              }
            ]
          }
        ],
        
          "rewrites": [
            {
              "source": "/fotos/:path*",
              "destination": "/fotos/:path*"
            }
          ]  
} 
